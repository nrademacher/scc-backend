{
  "name": "scc-backend",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma db push",
    "dev:prisma": "run-s prisma:*",
    "codegen": "graphql-codegen",
    "postcodegen": "file-ops prepend '// @ts-nocheck\n' src/types/graphql.ts",
    "dev": "concurrently \"ts-node-dev --no-notify --respawn --transpile-only src/servers\" \"yarn codegen --watch\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|prisma|graphql)\" --write",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "DB_ENV=test jest -i --detectOpenHandles --coverage",
    "util:copy-graphql-files-to-dist": "cpy '**/*.graphql' '../dist' --cwd=src --parents",
    "util:replace-dist-import-aliases": "file-ops fnr '#internal' '#internal_compiled' --silent 'dist/**/*.js'",
    "util:strip-dist-node-prefixes": "file-ops fnr 'node:' '' --silent 'dist/**/*.js'",
    "build": "yarn codegen && tsc",
    "postbuild": "run-s util:*",
    "start": "node dist/servers",
    "deploy": "prisma db push && concurrently \"yarn prisma studio\" \"yarn start\"",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "lint-staged": {
    "./{*,src/**/*}.{ts,prisma,graphql}": [
      "yarn format"
    ],
    "./{*,src/**/*}.ts": [
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-resolvers": "^2.5.2",
    "@nrademacher/file-ops-cli": "^0.6.8",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^17.0.21",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "husky": ">=6",
    "jest": "^27.5.1",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-prisma": "^3.10.0",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.4",
    "@graphql-tools/load": "^7.5.2",
    "@graphql-tools/schema": "^8.3.2",
    "@prisma/client": "^3.10.0",
    "apollo-server-express": "^3.6.3",
    "bcrypt": "^5.0.1",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "graphql-subscriptions": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "nanoid": "^3.3.4",
    "prisma": "^3.10.0",
    "subscriptions-transport-ws": "^0.11.0",
    "validator": "^13.7.0"
  },
  "imports": {
    "#internal/lib": "./src/lib/index.ts",
    "#internal/modules": "./src/modules/index.ts",
    "#internal/services": "./src/services/index.ts",
    "#internal/types": "./src/types/index.ts",
    "#internal/utils": "./src/utils/index.ts",
    "#internal/*": "./src/*.ts",
    "#internal_compiled/lib": "./dist/lib/index.js",
    "#internal_compiled/modules": "./dist/modules/index.js",
    "#internal_compiled/services": "./dist/services/index.js",
    "#internal_compiled/types": "./dist/types/index.js",
    "#internal_compiled/utils": "./dist/utils/index.js",
    "#internal_compiled/*": "./dist/*.js"
  }
}
